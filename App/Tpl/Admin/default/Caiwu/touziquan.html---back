  </div>
  
  <div class="list">
  <table id="area_list" width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <th style="width:30px;">
        <input type="checkbox" id="checkbox_handle" onclick="checkAll(this)" value="0">
        <label for="checkbox"></label>
    </th>
    <th class="line_l">用户ID</th>  
    <th class="line_l">用户名</th> 
    <th class="line_l">标号</th>
    <th class="line_l">投资金额</th>
    <th class="line_l">投资券使用金额</th>
    <th class="line_l">投资期限</th>
    <th class="line_l">投标日期</th>
    <th class="line_l">状态</th>
    <th class="line_l">订单号</th>
  
    <!-- <th class="line_l">操作</th> -->
  </tr>
  <volist id="vo" name="list">
      <tr overstyle='on' id="list_{$vo.uid}">
        <td><input type="checkbox" name="checkbox" id="checkbox2" onclick="checkon(this)" value="{$vo.uid}"></td>
        <td>{$vo.uid}</td>    
        <td>{$vo.user_phone}</td>  
         <td>{$vo.bid}</td>  
        <td>{$vo.invetmoney}</td>
        <td>{$vo.money}</td>
        <td>{$vo.timelimit}</td>
        <td><if condition="$vo.completetime eq null">--<else/>{$vo.completetime|date="Y-m-d H:i:s",###}</if></td>
        <td><php>if($vo['status']==2||$vo['status']==4){echo"投资成功";}else{echo "投资失败";}</php></td>
        <td>{$vo.order_no}</td>
      </tr>
  </volist>
